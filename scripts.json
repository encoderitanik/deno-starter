{
	"$schema": "https://deno.land/x/denon@2.5.0/schema.json",
	"allow": [
		"read",
		"env",
		"net",
		"write"
	],
	"scripts": {
		"start": {
			"cmd": "deno run --unstable --import-map=import_map.json server.ts",
			"unstable": false,
			"desc": "run server",
			"env": {
				"ENV": "development"
			},
			"watch": true,
			"tsconfig": "tsconfig.json"
		}
	}
}
